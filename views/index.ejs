<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>LotteryChecker</title>
</head>
<body>

    
    <% if(typeof mesg != "undefined") { %>   
        <h1><%= mesg %></h1>   
        <% } %>
    <h1>HomePAGE</h1>   
    <a href='/addlottery'>addLottery</a>   
    
    <table id=”table”>
    <table>
        <tr>
            <th>id</th>
            <th>DATE</th>
            <th>Number</th>
            <th>Prize</th>
        </tr>
        <% lottery.forEach(function(item) { %>
        <tr>
            <td><%= item.id %></td>
            <td><%= item.DATE %></td>
            <td><%= item.Number %></td>
            <td><%= item.Prize %></td>
        </tr>
        <% }) %>
    </table>
</body>
</html>